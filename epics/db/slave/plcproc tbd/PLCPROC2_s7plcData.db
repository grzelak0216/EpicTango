# # #################### LOCAL TIME ################################


#Czas ze sterownika PLC
record(ai, "$(P):s7:diLocalTime:plcproc2:ao"){
    field(DTYP, "S7plc")
    field(DESC, "Read local time")
    field(INP, "@PLC_PROC2_RCV/14 T=UINT32")
    field(SCAN, "I/O Intr")
}

#Czas z IOC
record(ao, "$(P):s7:diLocalTimeIOC:plcproc2:ai")
{
    field(DTYP, "S7plc")
    field(DESC, "Read local time IOC")
    field(OUT, "@PLC_PROC2_SND/6 T=UINT32")
    field(SCAN, "I/O Intr")
}

#Czas z IOC odbbity przez PLC
record(ai, "$(P):s7:diLocalTimeIOC:plcproc2:ao")
{
    field(DTYP, "S7plc")
    field(DESC, "Read local time IOC")
    field(INP, "@PLC_PROC2_RCV/10 T=UINT32")
    field(SCAN, "I/O Intr")
    field(FLNK, "$(P):ai1:plcproc2")
}
