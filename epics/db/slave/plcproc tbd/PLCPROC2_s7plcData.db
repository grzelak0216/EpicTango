### NOT CRITICAL

record(ao, "$(P):ao1:plcproc2") {
    field(DTYP, "Soft Channel")
    field(SCAN, "Passive")
    field(OUT, "$(P):s7:diLocalTimeIOC:plcproc2:ai PP")
    # field(FLNK, "$(P):ai1:plcproc2")
}

record(ao, "$(P):ai1:plcproc2") {
    field(DTYP, "Soft Channel")
    field(OMSL, "closed_loop")
    field(DOL, "$(P):s7:diLocalTimeIOC:plcproc2:ao PP")
    # field(OUT, "$(P):ai1:received PP")
    field(OUT, "$(P):ao1:notCritical:analog:received PP")
    field(SCAN, "Passive")
    # field(FLNK, "$(P):ai1:received PP")
}

#########################################################

record(bo, "$(P):bo1:plcproc2") {
    field(DTYP, "Soft Channel")
    field(ZNAM, "False")    
    field(ONAM, "True")
    field(SCAN, "Passive")
    field(OUT, "$(P):s7:singleBitToPlc:plcproc2 PP")
    # field(FLNK, "$(P):bi1:plcproc2")
}

record(bo, "$(P):bi1:plcproc2") {
    field(DTYP, "Soft Channel")
    field(ZNAM, "False")    
    field(ONAM, "True")
    field(OMSL, "closed_loop")
    field(DOL, "$(P):s7:singleBitFromPlc:plcproc2 PP")
    # field(OUT, "$(P):bi1:received PP")
    field(OUT, "$(P):bo1:notCritical:singlebit:received PP")
    field(SCAN, "Passive")
    # field(FLNK, "$(P):bi2:received PP")
}


