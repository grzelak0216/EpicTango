############### PLC COM ###############

# Single Bit from PLC to IOC
record (bi, "$(P):s7:singleBitFromPlc:plccom")
{	
    field(DTYP, "s7nodave")
	field(INP, "@PLC_COM DB2202.DBX28.0 bool")
	field(SCAN, "I/O Intr")
    field(DESC, "Single bit from PLC")
    field(ZSV, "MAJOR")
    field(ZNAM, "False")
    field(ONAM, "True")
    field(PINI, "YES")
}

# Single Bit from PLC to IOC
record (bo, "$(P):s7:singleBitToPlc:plccom")
{	
    field(DTYP, "s7nodave")
	field(OUT, "@PLC_COM DB2201.DBX12.1 bool")
    field(DESC, "Single bit to PLC")
    field(ZSV, "MAJOR")
    field(ZNAM, "False")
    field(ONAM, "True")
    field(PINI, "YES")
}

# Multi Bits from PLC to IOC
record (ai, "$(P):s7:multiBitsFromPlc:plccom")
{	
    field(DTYP, "s7nodave")
	field(INP, "@PLC_COM DB2202.DBD24 int32")
	field(SCAN, "I/O Intr")
    field(DESC, "Multiple bits from PLC")
    field(PINI, "YES")
}

# Multi Bits from IOC to PLC
