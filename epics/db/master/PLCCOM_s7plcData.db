####################### PLC COM ##########################


#Single Bit from PLC to IOC
record (bi, "$(P):s7:singleBitFromPlc:plccom")
{	
    field(DTYP, "S7plc")
	field(INP, "@PLC_COM_RCV/28 T=BYTE B=0")
	field(SCAN, "I/O Intr")
    field(DESC, "Single bit from PLC")
    field(ZSV, "MAJOR")
    field(ZNAM, "False")
    field(ONAM, "True")
    field(PINI, "YES")
}

#Single Bit from IOC to PLC
record (bo, "$(P):s7:singleBitToPlc:plccom")
{	
    field(DTYP, "S7plc")
    field(OUT, "@PLC_COM_SND/12 T=BYTE B=1")
    field(DESC, "Single bit to PLC")
    field(ZSV, "MAJOR")
    field(ZNAM, "False")
    field(ONAM, "True")
    field(PINI, "YES")
    field(SCAN, "I/O Intr")
}

#Multi Bits from PLC to IOC
record (ai, "$(P):s7:MultiBitsFromPlc:plccom")
{	
    field(DTYP, "S7plc")
	field(INP, "@PLC_COM_RCV/24 T=DWORD")
	field(SCAN, "I/O Intr")
    field(DESC, "Multi bits from PLC")
    field(PINI, "YES")
}

#Multi Bits from IOC to PLC
record (ao, "$(P):s7:MultiBitsToPlc:plccom")
{	
    field(DTYP, "S7plc")
    field(OUT, "@PLC_COM_SND/14 T=DWORD")
    field(DESC, "Multi bits to PLC")
	field(SCAN, "I/O Intr")
    field(PINI, "YES")
}


